import turtle
turtle.color("blue")
for i in range(4):
    turtle.forward(50)
    turtle.left(90)
turtle.left(90)
turtle.forward(50)
turtle.right(45)
turtle.forward(35)
turtle.right(90)
turtle.forward(35)
turtle.penup()
turtle.goto(0,0)
turtle.pendown()
turtle.left(45)
turtle.forward(15)
turtle.left(90)
turtle.forward(25)
turtle.right(90)
turtle.forward(20)
turtle.right(90)
turtle.forward(25)
turtle.penup()
turtle.forward(90)
turtle.write("home sweet home")
turtle.forward(90)
