import turtle
turtle.forward(100)
turtle.left(180)
turtle.forward(100)
turtle.right(90)
turtle.forward(100)
turtle.penup()
turtle.goto(0,0)
turtle.pendown()
turtle.right(90)
turtle.forward(10)
turtle.left(90)
turtle.forward(32)
turtle.right(90)
turtle.forward(20)
turtle.right(90)
turtle.forward(32)

turtle.left(90)
turtle.forward(10)
turtle.left(90)
turtle.forward(6)
turtle.right(90)
turtle.forward(20)
turtle.right(90)
turtle.forward(6)

turtle.left(90)
turtle.forward(10)
turtle.left(90)
turtle.forward(95)
turtle.right(90)
turtle.forward(20)
turtle.right(90)
turtle.forward(95)

turtle.penup()
turtle.goto(123,123)
turtle.write("Bar Chart:")
turtle.goto(-123,-123)
